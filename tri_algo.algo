ALGORITHM Deuxieme_tache
VAR
    arr : ARRAY_OF type[length];
    i,j,k: INTEGER //sachant que k est notre clÃ©
BEGIN
   i:= 0
   FOR i FROM 0 TO arr.length-1  DO
    k := arr[i]
    j := i-1
    WHILE (arr[j] > k) AND (j >= 0) DO
    arr[j+1]:= arr[j]
    j := j - 1
    ENDWHILE
    arr[j + 1] := k
   END_FOR
END